page {
    height: 100%;
    width: 100%;
    color: #212330;
    font-size: 28rpx;
    background-color: #f3f3f3;
    font-family: "PingFang SC","Source Han Sans CN";
}

buttom {
    font-size: 28rpx;
}

.f-r {
    color: #ff0000;
}

view {
    box-sizing: border-box;
}

.icon_r {
    width: 14rpx;
    height: 25rpx;
}

.center {
    display: flex;
    justify-content: center;
    align-items: center;
}

.around {
    display: flex;
    flex-direction: row;
    justify-content: space-around;
}

.center-c {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.center-r {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.between {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.betweens {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}

.row {
    display: flex;
    flex-direction: row;
}

.rows {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.column {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.flex-c {
    display: flex;
    flex-direction: column;
}

.columns {
    display: flex;
    flex-direction: column;
}

.z-index {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    background-color: #000;
    background: rgba(0,0,0,0.3);
    z-index: 100;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.icon-img {
    width: 50rpx;
    height: 50rpx;
}

.no-row {
    word-break: keep-all;
    white-space: nowrap;
}

.no-rows {
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.w-h {
    width: 50%;
}

.w-f {
    width: 100%;
}

.full {
    height: 100%;
    width: 100%;
}

.price-question {
    width: 17rpx;
    height: 17rpx;
    margin-left: 10rpx;
}

.property-r-img {
    width: 10rpx;
    height: 19rpx;
    margin-left: 20rpx;
}

.view-box {
    width: 100%;
    height: 20rpx;
    background: #f3f3f3;
}

.no-hint-font {
    color: #212330;
    font-size: 24rpx;
}

.f-w {
    font-weight: bold;
}