page {
    background: #ffffff;
}

.column {
    display: flex;
    flex-direction: column;
}

.center {
    display: flex;
    align-items: center;
    justify-content: center;
}

.row {
    display: flex;
    flex-direction: row;
    align-items: center;
    flex-wrap: wrap;
}

.between {
    justify-content: space-between;
}

.item_list {
    height: 104rpx;
    border-bottom: 1rpx solid #ebebeb;
}

.register_content {
    margin: 20rpx;
    padding: 20rpx;
    width: 710rpx;
}

.item_info {
    margin-bottom: 46rpx;
}

.item_list_input {
    text-align: right;
}

.item_list_title {
    font-size: 36rpx;
    font-weight: bold;
}

.car_type {
    height: 80rpx;
    padding: 0 14rpx;
    border-radius: 15rpx;
    margin-bottom: 15rpx;
    margin-right: 20rpx;
    display: flex;
    justify-content: space-around;
    color: #808088;
    border: 1px solid #cecece;
}

.pick {
    background: #ffd101;
    color: #2c282e;
    border: 1px solid transparent;
    font-weight: bold;
}

.car_type:nth-of-type(3n+0) {
    margin-right: 0rpx;
}

.carID {
    margin-top: 70rpx;
    margin-left: -20rpx;
    margin-right: -20rpx;
}

.carid_item {
    width: 330rpx;
    height: 200rpx;
    border: 1rpx solid #ebebeb;
    border-radius: 15rpx;
    margin-bottom: 30rpx;
}

.carid_text {
    display: flex;
    justify-content: center;
    align-content: center;
    font-weight: bold;
}

.carid_box {
    padding-bottom: 30rpx;
}

.hint {
    font-size: 24rpx;
    color: #808088;
}

.submit_bth {
    font-size: 36rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    width: 670rpx;
    height: 90rpx;
    background: #ffd101;
    border-radius: 50rpx;
}

.icon_center {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 40rpx;
    margin-right: 14rpx;
}

.icon_wh {
    width: 55rpx;
    height: 40rpx;
}

.box-transition {
    transition: all 0.5s;
}