page {
    background: #f0f2f5;
}

.top_background {
    height: 200rpx;
    background: #2c282e;
}

.wrow {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.wcolumn {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.waround {
    justify-content: space-around;
}

.wcenter {
    justify-content: center;
}

.witcolumn {
    display: flex;
    flex-direction: column;
    align-content: flex-start;
    padding-left: 38rpx;
    margin-top: 40rpx;
}

.wallet_centent {
    width: 710rpx;
    position: absolute;
    top: 60rpx;
    left: 20rpx;
    right: 20rpx;
}

.mid_balance {
    height: 330rpx;
    background: url(data:image/png;base64,) no-repeat center;
    border-radius: 15rpx;
}

.balance_price_text {
    font-size: 90rpx;
    font-weight: bold;
}

.balance_unit_text {
    font-size: 28rpx;
    font-weight: bold;
}

.two_button {
    height: 120rpx;
    background: #ffffff;
    border-bottom-right-radius: 15rpx;
    border-bottom-left-radius: 15rpx;
    font-size: 28rpx;
    font-weight: bold;
}

.two_button_icon {
    width: 25rpx;
    height: 30rpx;
}

.two_button_text {
    font-size: 28rpx;
    padding-left: 25rpx;
}

.withdrawal {
    margin-top: 25rpx;
    border-radius: 15rpx;
    background: #ffffff;
}

.wline {
    height: 12rpx;
    border-top-right-radius: 15rpx;
    border-top-left-radius: 15rpx;
    background: #ffd101;
}

.input_price {
    margin-top: 40rpx;
    padding-bottom: 40rpx;
}

.input_price input {
    margin-left: 60rpx;
    border-color: #e7e7e7;
    border-style: solid;
    border-top-width: 0px;
    border-right-width: 0px;
    border-bottom-width: 1px;
    border-left-width: 0px;
}

.withdrawal_font {
    font-size: 24rpx;
    color: #808088;
}

.withdrawal_type {
    width: 160rpx;
    height: 56rpx;
    margin-top: 60rpx;
    background: #ffd101;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: 15rpx;
    font-size: 27rpx;
    margin-bottom: 60rpx;
}

.allBtn {
    width: 100rpx;
    height: 50rpx;
    color: #ffffff;
    font-size: 24rpx;
    background: #ffd101;
    border-radius: 35px;
    margin-left: 40rpx;
}

.all-hover {
    background: rgba(255,209,1,0.5);
}

.whint {
    margin-top: 60rpx;
    margin-left: 20rpx;
    font-size: 20rpx;
    color: #bdbdbd;
}

.btn {
    width: 680rpx;
    height: 90rpx;
    border-radius: 35px;
    font-size: 36rpx;
}

.driver-btn {
    background: #ffd101;
    margin-top: 70rpx;
}